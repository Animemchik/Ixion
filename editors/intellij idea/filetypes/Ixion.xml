<filetype binary="false" default_extension="ix" description="Ixion files" name="Ixion">
  <highlighting>
    <options>
      <option name="LINE_COMMENT" value="//" />
      <option name="COMMENT_START" value="/*" />
      <option name="COMMENT_END" value="*/" />
      <option name="HEX_PREFIX" value="0x" />
      <option name="NUM_POSTFIXES" value="" />
      <option name="HAS_BRACES" value="true" />
      <option name="HAS_BRACKETS" value="true" />
      <option name="HAS_PARENS" value="true" />
      <option name="HAS_STRING_ESCAPES" value="true" />
    </options>
    <keywords keywords="break;case;continue;do;else;for;if;length;println;return;throw;while" ignore_case="false" />
    <keywords2 keywords="catch;class;def;enum;ext;getter;new;override;priv;pub;setter;static;try;using" />
    <keywords3 keywords="false;null;this;to;true" />
    <keywords4 keywords="String;byte;char;const;double;float;int;long;short;var" />
  </highlighting>
  <extensionMap>
    <mapping ext="ix" />
  </extensionMap>
</filetype>